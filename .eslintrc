{
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "extends": ["airbnb", "airbnb/hooks"],
    "rules": {
      # override rules here
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/prop-types": 0,
      "react/no-array-index-key": 0,
      "no-console": 0
    }
}
